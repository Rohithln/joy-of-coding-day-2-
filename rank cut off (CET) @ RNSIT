#include<stdio.h>
int main()
{
    int rank;
    printf("Enter rank: ");
    scanf("%d",&rank);
    if(rank<3251)
    printf("Admission in all branches is possible");
    else if(rank>3250&&rank<6506)
    printf("Adimission in ISE, EandC, MEC branch");
    else if(rank>6505&&rank<12013)
    printf("Adimission in EandC, MEC branch");
    else if(rank>12012&&rank<22341)
    printf("Adimission in MEC branch");
    else
    printf("Admission not possible");
    return 0;
}
